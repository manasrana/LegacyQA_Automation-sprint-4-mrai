<?xml version="1.0" encoding="UTF-8"?>
<suite name="Sprint" verbose="1" >
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Add New Line flow" >
        <parameter name="test-type" value="internal" />
        <classes>
            <class name="sprint.tests.AddNewLineTests" />
            <methods>
                <include name="QA_4242_NewActivationWithEasyPay"/>
                <include name="QA_4248_SprintEasyPay_CancelOrder_WithOutAccepting_Contract"/>
                <include name="QA_3928_SprintNonEasyPayWithNewActivation_FamilySharePackPlan"/>
                <include name="QA_85_SPT2YrContract_CancelOrderBeforePayment"/>
                <include name="QA_2966_SprintNewActivationWithEasyPay"/>
                <include name="QA_1705_Sprint2YrContractWithNewActivation"/>
                <include name="QA_93_Sprint2YrAALExistingFamilyPlan"/>
            </methods>
        </classes>
    </test>
    <test name="Upgrade Tests">
        <parameter name="test-type" value="internal" />
        <classes>
            <class name="sprint.tests.UpgradeTests" />
            <methods>
                <include name="QA3096_BuddyUpgradeMultipleLinesEligibleOneUpgradeEligibleOneUpgradeIneligible"/>
                <include name="QA_3181_SprintAddaLineCancelOnInstallmentDetails"/>
                <include name="QA_495_Sprint2YrAALExistingFamilyPlan"/>
